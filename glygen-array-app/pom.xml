		</dependency -->
		<!-- 
		<dependency>
		  <groupId>eurocarb</groupId>
		  <artifactId>eurocarb-molecularframework</artifactId>
		  <version>1.0rc</version>
		</dependency>  -->
		<dependency>
		  <groupId>resourcesdb</groupId>
		  <artifactId>resourcesdb_interfaces</artifactId>
		  <version>1</version>
		</dependency>
		<!--  GlycoInfo -->
		<dependency>
            <groupId>org.glycoinfo.convert</groupId>
            <artifactId>glycanformatconverter</artifactId>
            <version>2.8.2</version>
        </dependency>
		<dependency>
		  <groupId>glycoinfo</groupId>
		  <artifactId>molecular</artifactId>
		  <version>0.1.37</version>
		</dependency>
		<!-- <dependency>
		  <groupId>glycoinfo</groupId>
		  <artifactId>wurcs</artifactId>
		  <version>1.0.1</version>
		</dependency> -->
		<dependency>
		  <groupId>org.glycoinfo</groupId>
		  <artifactId>wurcsframework</artifactId>
		  <version>1.2.9</version>
		</dependency>
		<!-- https://mvnrepository.com/artifact/org.jdom/jdom -->
		<dependency>
		    <groupId>org.jdom</groupId>
		    <artifactId>jdom</artifactId>
		    <version>1.1.3</version>
		</dependency>
		<!-- https://mvnrepository.com/artifact/org.apache.logging.log4j/log4j-core/2.16.0 -->
		<dependency>
		    <groupId>org.apache.logging.log4j</groupId>
		    <artifactId>log4j-api</artifactId>
		    <version>2.16.0</version>
		</dependency>
	    <dependency>
		    <groupId>org.apache.logging.log4j</groupId>
		    <artifactId>log4j-core</artifactId>
		    <version>2.16.0</version>
		</dependency>
		<!-- https://mvnrepository.com/artifact/org.apache.logging.log4j/log4j-1.2-api -->
